{
  "expo": {
    "name": "sparrow",
    "slug": "sparrow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "mainColors":{
      "primaryColor": "#FF8F1F",
      "secondaryColor": "#a31d20",
      "darkGray": "#7b8591",
      "lightGray": "#f6f6fa",
      "backgroundGray": "#e5e5e5",
      "black": "#101611",
      "white": "#ffffff",
      "red": "#ff0000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
